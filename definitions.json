[
    {
        "name": "usermgmt_service",
        "active": true,
        "proxy": {
            "listen_path": "/usrmgmt/*",
            "upstreams": {
                    "strategy": "rr",
                    "targets": ["http://localhost:8080", "http://localhost:8081"]
            },
            "strip_path": true,
            "preserve_host": false
        },
        "plugins": [
            {
                "name":"limiter",
                "enable": true,
                "config": {
                    "strategy": "sliding-window",
                    "storage": "local",
                    "policy": "IP",
                    "rates": ["5/1-M"]
                }
            }
        ]
    },
    {
        "name": "test",
        "active": true,
        "proxy":{
            "listen_path": "/test/*",
            "upstreams": {
                    "strategy": "rr",
                    "targets": ["http://localhost:8080"]
                }
        },
        "plugins": [
            {
                "name":"limiter",
                "enable": false,
                "config": {
                    "quota": 4,
                    "per": "m"
                }
            }
        ]
    }
]
